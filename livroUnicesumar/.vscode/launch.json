            ]
        },
        {
            "name": "(gdb) Anexação de Pipe",
            "type": "cppdbg",
            "request": "attach",
            "program": "insira o nome do programa, por exemplo ${workspaceFolder}/a.out",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "/usr/bin/ssh",
                "pipeArgs": [],
                "pipeCwd": ""
            },
            "MIMode": "gdb",
            "setupCommands": [
